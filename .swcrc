{
  "minify": true,
  "jsc": {
    "baseUrl": "src",
    "paths": {
      "@schema/*": ["schema/*"],
      "@middlewares/*": ["middlewares/*"],
      "@generated/*": ["generated/*"],
      "@config/*": ["config/*"],
      "@lib/*": ["lib/*"],
      "@services/*": ["services/*"],
      "@models/*": ["models/*"],
      "@controllers/*": ["controllers/*"],
      "@interfaces/*": ["interfaces/*"],
      "@ts/*": ["ts/*"]
    },
    "parser": {
      "syntax": "typescript"
    },
    "target": "es2021",
    "minify": {
      "compress": true,
      "mangle": true
    }
  },
  "module": {
    "type": "commonjs"
  }
}
